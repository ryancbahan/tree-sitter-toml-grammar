{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "toml",
      "camelcase": "Toml",
      "title": "Toml",
      "scope": "source.toml",
      "file-types": [
        "toml"
      ],
      "injection-regex": "^toml$",
      "class-name": "TreeSitterToml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Toml grammar for tree-sitter",
    "authors": [
      {
        "name": "Ryan Bahan"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-toml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}